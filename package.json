{
  "name": "wiplock",
  "private": true,
  "version": "0.0.1",
  "description": "Wiplock protects mistakenly merging pull requests in progress",
  "main": "assets/src/index.js",
  "scripts": {
    "webpack": "webpack",
    "build": "npm run webpack && npm run bindata && npm run go:build",
    "eslint": "eslint .",
    "go:bindata": "go-bindata -pkg app -o ./app/bindata.go assets/build",
    "go:build": "go build -v",
    "go:deps": "go get -u github.com/jteeuwen/go-bindata/... && go get -t -d -v",
    "go:test": "go test -v ./... | sed '/PASS/s//$$(printf \"\\033[32mPASS\\033[0m\")/' | sed '/FAIL/s//$$(printf \"\\033[31mFAIL\\033[0m\")/'",
    "lint": "npm run eslint && npm run stylint",
    "postinstall": "npm run godeps",
    "start": "webpack-dev-server",
    "stylint": "stylint .",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngs/wiplock.git"
  },
  "keywords": [
    "github",
    "wip",
    "pull",
    "request",
    "workflow"
  ],
  "author": "Atsushi Nagase",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ngs/wiplock/issues"
  },
  "homepage": "https://github.com/ngs/wiplock#readme",
  "devDependencies": {
    "autoprefixer": "^6.4.1",
    "babel-cli": "^6.14.0",
    "babel-core": "^6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.5",
    "babel-polyfill": "^6.13.0",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-es2016": "^6.11.3",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "bootstrap-styl": "^5.0.7",
    "classnames": "^2.2.5",
    "css-loader": "^0.25.0",
    "css-mqpacker": "^5.0.1",
    "eslint": "^3.5.0",
    "eslint-loader": "^1.5.0",
    "eslint-plugin-react": "^6.2.2",
    "event-emitter": "^0.3.4",
    "file-loader": "^0.9.0",
    "flux": "^2.1.1",
    "history": "^4.0.1",
    "jest-cli": "^15.1.1",
    "keymirror": "^0.1.1",
    "nib": "^1.1.2",
    "object-assign": "^4.1.0",
    "postcss-flexboxfixer": "0.0.5",
    "postcss-loader": "^0.13.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-fa": "^4.1.2",
    "react-router": "^2.8.1",
    "react-router-bootstrap": "^0.23.1",
    "style-loader": "^0.13.1",
    "stylint": "^1.5.6",
    "stylint-loader": "^1.0.0",
    "stylint-stylish": "^1.3.2",
    "stylus-loader": "^2.3.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.15.2",
    "whatwg-fetch": "^1.0.0"
  },
  "jest": {
    "testPathDirs": [
      "<rootDir>/asserts/src"
    ],
    "cacheDirectory": "<rootDir>/cache/haste",
    "moduleNameMapper": {
      "^.+\\.(css|less|styl|sass|svg|png|mp3)$": null
    },
    "verbose": true
  }
}
